{
    "name": "Video Stats Overlay",
    "description": "Overlay on video elements, displaying media debug stats",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ]
  }
